{
  "name": "@elizaos/eliza-starter",
  "version": "0.1.9",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "start": "tsc && node --loader ts-node/esm src/index.ts",
    "clean": "./scripts/clean.sh",
    "start:service": "pm2 start pnpm --name=elizaArthera --restart-delay=3000 --max-restarts=10 -- run start",
    "stop:service": "pm2 stop elizaArthera"
  },
  "dependencies": {
    "@elizaos/adapter-sqlite": "0.1.9",
    "@elizaos/client-direct": "^0.1.9",
    "@elizaos/client-telegram": "0.1.9",
    "@elizaos/core": "0.1.9",
    "@elizaos/plugin-arthera": "^0.1.9",
    "@elizaos/plugin-bootstrap": "0.1.9",
    "@elizaos/plugin-node": "0.1.9",
    "@tavily/core": "0.0.2",
    "amqplib": "0.10.5",
    "better-sqlite3": "11.5.0",
    "fs": "0.0.1-security",
    "net": "1.0.2",
    "path": "0.12.7",
    "readline": "1.3.0",
    "url": "0.11.4",
    "ws": "8.18.0",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "1.20.0"
    }
  },
  "devDependencies": {
    "pm2": "5.4.3",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "typescript": "5.6.3"
  },
  "packageManager": "pnpm@9.15.2+sha512.93e57b0126f0df74ce6bff29680394c0ba54ec47246b9cf321f0121d8d9bb03f750a705f24edc3c1180853afd7c2c3b94196d0a3d53d3e069d9e2793ef11f321"
}
